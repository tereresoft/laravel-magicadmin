{
    "name": "rifarca/magicadmin",
    "description": "Crea un Admin Backend para ABM con dentro de Laravel con Theme de Bootstrap",
    "authors": [
        {
            "name": "rifarca",
            "email": "rifarca@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://localhost/magicadmin"
        }
    ],
    "minimum-stability": "stable",
    "scripts":{
			"post-root-package-install": [
				"php -r \"copy('MagicAdminCommand.php', 'app/Console/Commands/MagicAdminCommand.php');\""
			]
	},
    "config": {
        "disable-tls": true,
        "secure-http": false
    }
}
